<?xml version="1.0" encoding="UTF-8"?>
<data
	xmlns="https://www.woltlab.com"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/6.0/option.xsd"
>
	<import>
		<categories>
			<category name="assets" />
			<category name="assets_general">
				<parent>assets</parent>
			</category>
			<category name="assets_pdf_export">
				<parent>assets</parent>
			</category>
			<category name="assets_label">
				<parent>assets</parent>
			</category>
			<category name="assets_audit">
				<parent>assets</parent>
			</category>
		</categories>
		<options>
			<option name="assets_install_time">
				<categoryname>assets_general</categoryname>
				<optiontype>integer</optiontype>
				<hidden>1</hidden>
			</option>
			<option name="assets_legacyid_enabled">
				<categoryname>assets_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="assets_export_format">
				<categoryname>assets_pdf_export</categoryname>
				<optiontype>AssetsExportPaperSize</optiontype>
				<defaultvalue>a4</defaultvalue>
			</option>
			<option name="assets_export_orientation">
				<categoryname>assets_pdf_export</categoryname>
				<optiontype>select</optiontype>
				<defaultvalue>portrait</defaultvalue>
				<selectoptions>landscape:wcf.acp.option.assets_export_orientation.landscape
portrait:wcf.acp.option.assets_export_orientation.portrait</selectoptions>
			</option>
			<option name="assets_label_header">
				<categoryname>assets_label</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>Assets</defaultvalue>
			</option>
			<option name="assets_label_logo">
				<categoryname>assets_label</categoryname>
				<optiontype>file</optiontype>
				<filelocation>images/label_logo</filelocation>
				<fileExtensions>jpeg
jpg
png
webp</fileExtensions>
			</option>
			<option name="assets_label_logo_height">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>1.0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_logo_margin_top">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.05</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_logo_margin_right">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.05</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_per_page">
				<categoryname>assets_label</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>24</defaultvalue>
			</option>
			<option name="assets_label_format">
				<categoryname>assets_label</categoryname>
				<optiontype>AssetsExportPaperSize</optiontype>
				<defaultvalue>a4</defaultvalue>
			</option>
			<option name="assets_label_orientation">
				<categoryname>assets_label</categoryname>
				<optiontype>select</optiontype>
				<defaultvalue>landscape</defaultvalue>
				<selectoptions>landscape:wcf.acp.option.assets_export_orientation.landscape
portrait:wcf.acp.option.assets_export_orientation.portrait</selectoptions>
			</option>
			<option name="assets_label_page_margin_top">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.5</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_page_margin_right">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_page_margin_bottom">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.5</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_page_margin_left">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_font_size">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.2</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_width">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>5.75</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_height">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>2.5</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_row_gap">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>1.2</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_column_gap">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>1.2</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_qr_width">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>2.4</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_qr_height">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>2.4</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_qr_padding_right">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_qr_padding_top">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_title_margin_start">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.1</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_title_margin_end">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.15</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_text_margin_start">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.1</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_text_margin_end">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0.1</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_text_padding_top">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_text_padding_right">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_text_padding_bottom">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_label_text_padding_left">
				<categoryname>assets_label</categoryname>
				<optiontype>float</optiontype>
				<defaultvalue>0</defaultvalue>
				<minvalue>0</minvalue>
				<suffix>cm</suffix>
			</option>
			<option name="assets_audit_interval">
				<categoryname>assets_audit</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<enableoptions>assets_audit_date</enableoptions>
			</option>
			<option name="assets_auditinterval_amount">
				<categoryname>assets_audit</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="assets_auditinterval_unit">
				<categoryname>assets_audit</categoryname>
				<optiontype>select</optiontype>
				<defaultvalue>Y</defaultvalue>
				<selectoptions>Y:wcf.acp.option.assets_auditinterval_unit.year
M:wcf.acp.option.assets_auditinterval_unit.month
W:wcf.acp.option.assets_auditinterval_unit.week
D:wcf.acp.option.assets_auditinterval_unit.day</selectoptions>
			</option>
			<option name="assets_audit_date">
				<categoryname>assets_audit</categoryname>
				<optiontype>date</optiontype>
			</option>
		</options>
	</import>
</data>
